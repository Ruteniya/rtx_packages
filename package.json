{
  "name": "rtxtypes",
  "version": "1.1.9",
  "description": "Storage for types that are common for Client and Server of RtX",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "release": "standard-version",
