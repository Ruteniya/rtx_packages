  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ruteniya/RtX_Packages/issues"
  },
  "homepage": "https://github.com/Ruteniya/RtX_Packages#readme",
  "devDependencies": {
    "prettier": "^3.5.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
